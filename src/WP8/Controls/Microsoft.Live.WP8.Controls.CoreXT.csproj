  <Target Name="BeforeBuild">
  </Target>
  <Target Name="AfterBuild">
  </Target>
  -->
  <Target Name="CallSilo" AfterTargets="AfterCompile">
    <!-- We must sign Silverlight DLLs that have dependencies right after they are compiled. -->
    <Exec Command="set complus_installroot=&amp;set complus_version=&amp;$(Silo) $(SignType) $(SourcePath)\Managed\SignOneBinary.xml SourcePath=$(IntermediateOutputPath) DestPath=$(SignedOutputTargetPath)\WP8\buildtime FileName=$(OutputFileName) PrsFilePath=$(ManagedRoot)" />

    <!-- Must copy signed DLL into intermediate output directory for ship build sign verification. -->
    <Copy SourceFiles="$(SignedOutputTargetPath)\WP8\buildtime\$(OutputFileName)" DestinationFolder="$(IntermediateOutputPath)" />
  </Target>
